{
  "name": "router-headers-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "dev": "concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/sensible": "^5.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.81",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.0.0",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "20.5.7",
    "tap": "^16.1.0",
    "typescript": "5.2.2"
  }
}
